{
    "source": {
        "include": ["src", "README.md"]
    },
     "templates": {
      "default": {
        "outputSourceFiles": true
      }
    },
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "tutorials": "./tutorials",
        "theme_opts": {
            "theme": "dark"
        }
    }
}